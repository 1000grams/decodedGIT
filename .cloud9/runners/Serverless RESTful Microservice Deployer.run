{
  "script": [
    "for directory in /projects/* ; do",
    "  if [ -d \"$directory/.cloud9/runners\" ]; then",
    "    REPOSITORY_DIR=$directory",
    "  fi",
    "done",
    "echo $REPOSITORY_DIR",
    "cd $REPOSITORY_DIR",
    "npm run cdk deploy"
  ],
  "info" : "This runner will deploy Serverless RESTful Microservice project to AWS."
}
