{
  "script": [
    "for directory in /projects/* ; do",
    "  if [ -d \"$directory/.cloud9/runners\" ]; then",
    "    REPOSITORY_DIR=$directory",
    "  fi",
    "done",
    "echo $REPOSITORY_DIR",
    "cd $REPOSITORY_DIR",
    "npm test"
  ],
  "info" : "This runner will execute unit tests."
}
